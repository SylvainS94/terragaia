<form method="post" action="{{ path('app_theme_delete', {'id': theme.id}) }}" onsubmit="return confirm('Etes vous certain de vouloir supprimer définitivement ce ce Thème Qui suis-je ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ theme.id) }}">
    <button class="btn btn-danger"><i class="fa fa-trash fa-beat mx-2"></i>Supprimer</button>
</form>
