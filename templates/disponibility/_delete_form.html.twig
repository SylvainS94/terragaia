<form method="post" action="{{ path('app_disponibility_delete', {'id': disponibility.id}) }}" onsubmit="return confirm('Etes vous certain de vouloir supprimer définitivement cette disponibilité ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ disponibility.id) }}">
    <button class="btn btn-danger"><i class="fa fa-trash fa-beat mx-2"></i>Supprimer</button>
</form>